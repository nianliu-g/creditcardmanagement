{
  "name": "ps-redux",
  "description": "A credit card management system built by Nian Liu",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js --port 3000"
  },
  "dependencies": {
    "bootstrap": "4.3.1",
    "immer": "2.1.3",
    "prop-types": "15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "6.0.1",
    "react-router-dom": "^5.0.0",
    "react-toastify": "4.5.2",
    "redux": "4.0.1",
